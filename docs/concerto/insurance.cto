namespace org.codex.insurance

concept Amount {
  o String quantity
  o UnitEnum unit
  o UnitDimensionEnum unitDimension
}

concept ChainCompositionOr extends Chainable {
  o RiskChain[] operands
}

concept ChainOperator extends Chainable {
  o ChainOperatorEnum type
}

enum ChainOperatorEnum {
  o wildcard
  o not
}

abstract concept Chainable {
}

concept InsurancePayout {
  o PayoutTypeEnum payoutType
  o Amount[] amount
  o Amount[] minimalAmount
  o Multiplier multipliers
  o RiskChain for
  o Boolean impactOtherConsumption
}

concept Multiplier {
  o MultiplierTimeEnum time
  o MultiplierPersonEnum person
  o MultiplierOccurenceEnum occurence
  o MultiplierInsuredObjectEnum insuredObject
  o String[] other
}

enum MultiplierInsuredObjectEnum {
  o perCar
}

enum MultiplierOccurenceEnum {
  o perClaim
  o perLine
  o perIncident
}

enum MultiplierPersonEnum {
  o perPerson
  o perPassenger
  o perEmployee
  o perLoneTraveler
}

enum MultiplierTimeEnum {
  o perMinutes
  o perHour
  o perDay
  o perMonth
  o perCalendarMonth
  o perYear
  o perPeriodOfInsurance
}

enum PayoutTypeEnum {
  o Limit
  o Excess
  o Deductible
  o Franchise
  o StrictLimit
  o Exclusion
}

concept Person extends RiskObject {
  o Boolean isPolicyHolder
  o String firstname
  o String lastname
  o Double age
}

concept Relation {
  o String impacterId
  o String impacteeId
  o String verb
}

concept RiskChain {
  o Chainable[] chain
  o RiskObject[] actors
  o Relation[] relations
}

abstract concept RiskObject {
  o String id
}

concept Situation extends Chainable {
  o String id
  o String event
  o String location
  o String period
}

